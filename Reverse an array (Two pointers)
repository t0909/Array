//------------------O(n2)-------------------------------------
#include<bits/stdc++.h>
using namespace std;

void reverse(int arr[], int n)
{
    
   int i=0;
   int j=n-1;
   while(i<j)
   {
       swap(arr[i],arr[j]);
       i++;
       j--;
   }
    
    
}

int main()
{
   int arr[]={800,100,29,20,5,5};
   
   reverse(arr,6);
   
   for(int i=0;i<6;i++)
   cout<<arr[i]<<" ";
   
   
    
    
}

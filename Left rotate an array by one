

#include<bits/stdc++.h>
using namespace std;



void lrotateOne(int arr[], int n)
{
    
  int temp = arr[0];
  
  for(int i=0; i<n-1; i++)
  {
      arr[i]=arr[i+1];
  }
  
  arr[n-1]=temp;
}

int main()
{
   int arr[]={800,0,0,20,51,5};
   
  lrotateOne(arr,6);
   
   for(int i=0; i<6; i++)
   cout<<arr[i]<<" ";
   
   
    
    
}

#include<bits/stdc++.h>
using namespace std;

int greatestElement(int arr[], int n)
{
    int res=0;
    for(int i=1; i<n; i++)
    {
        if(arr[i]>arr[res])
        {
            arr[res]=arr[i];
        }
    }
    
    return res;
}

int main()
{
    int arr[]={2,5,8,1,90,100};
   int p= greatestElement(arr,6);
   cout<<arr[p];
    
    
}

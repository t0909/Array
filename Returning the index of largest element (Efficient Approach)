#include<bits/stdc++.h>   //o(n)
using namespace std;

int greatestElement(int arr[], int n)
{
    int res=arr[0];
    for(int i=1; i<n; i++)
    {
        if(arr[i]>res)
        {
            res=arr[i];
        }
    }
    
    return res;
}

int main()
{
    int arr[]={2,5,8,1,90,100};
   cout<< greatestElement(arr,6);
    
    
}

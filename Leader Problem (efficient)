
// theta(n)

#include<bits/stdc++.h>
using namespace std;

void leader(int arr[], int n)
{
   
    
   int curr_leader= arr[n-1];
   cout<<curr_leader<<" ";
   
   for(int i=n-2; i>=0; i--)
   {
       if(arr[i]>curr_leader)
       {
           
           curr_leader = arr[i];
           cout<<curr_leader<<" ";
           
       }
   }
}





int main()
{
   int arr[]={800,0,2,20,51,5};
   
  leader(arr,6);
   
   
   
    
    
}
